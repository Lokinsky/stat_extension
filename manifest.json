
{
    "manifest_version": 2,
    "name": "Stats",
    "version": "1.0",
    
    "icons": {
      "128": "/Diffuse.jpg"
    },
    "content_scripts": [
      {
        "matches": [ "*://*/*" ],
        "js": [ "content.js" ]
      }
    ],
    "background": {
        "scripts":["background.js"]

    },
    "content_security_policy": "script-src 'self' unsafe-inline 'sha256-WOdSzz11/3cpqOdrm89LBL2UPwEU9EhbDtMy2OciEhs='",
    "permissions": [
      "api.coincap.io/v2/assets"
    ],
  
        "browser_action": {
          "default_title": "Stats",
          "default_icon": "/Diffuse.jpg",
          "default_popup": "popup.html"
      }
      
  }